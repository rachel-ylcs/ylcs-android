<?xml version="1.0" encoding="utf-8"?>
<resources>
    <attr name="BackgroundColor" format="color" />
    <attr name="BorderColor" format="color" />
    <attr name="TitleColor" format="color" />
    <attr name="ValueColor" format="color" />

    <attr name="BorderWidth" format="dimension" />
    <attr name="Gap" format="dimension" />
    <attr name="IconSize" format="dimension" >
        <enum name="wrap_content" value="-2" />
    </attr>
    <attr name="ItemPaddingX" format="dimension" />
    <attr name="ItemPaddingY" format="dimension" />

    <attr name="AssetName" format="string" />
    <attr name="Home" format="string" />
    <attr name="Title" format="string" />
    <attr name="Value" format="string" />

    <attr name="Column" format="integer" />
    <attr name="Duration" format="integer" />
    <attr name="MaxFileSize" format="integer" />
    <attr name="MaxNum" format="integer" />
    <attr name="Quality" format="integer" />

    <attr name="Parallax" format="float" />
    <attr name="Ratio" format="float" />

    <attr name="Bold" format="boolean" />
    <attr name="Expanded" format="boolean" />
    <attr name="HasArrow" format="boolean" />

    <declare-styleable name="AspectRatioImageView" >
        <attr name="Ratio" />
    </declare-styleable>
    <declare-styleable name="AvatarView" >
        <attr name="BorderWidth" />
        <attr name="BorderColor" />
        <attr name="BackgroundColor" />
    </declare-styleable>
    <declare-styleable name="BreadCrumbView" >
        <attr name="Home" />
    </declare-styleable>
    <declare-styleable name="ButtonGroup" >
        <attr name="ButtonGroupTextPosition" format="enum" >
            <enum name="BOTTOM" value="1" />
            <enum name="TOP" value="2" />
        </attr>
        <attr name="ButtonGroupItemGravity" format="enum" >
            <enum name="WRAP" value="1" />
            <enum name="AVERAGE" value="2" />
        </attr>
        <attr name="ButtonGroupItemGap" format="dimension" />
        <attr name="android:textSize" />
        <attr name="android:color" />
        <attr name="Bold" />
        <attr name="Gap" />
        <attr name="ButtonGroupIcon1" format="reference" />
        <attr name="ButtonGroupIcon2" format="reference" />
        <attr name="ButtonGroupIcon3" format="reference" />
        <attr name="ButtonGroupIcon4" format="reference" />
        <attr name="ButtonGroupIcon5" format="reference" />
        <attr name="ButtonGroupIcon6" format="reference" />
        <attr name="ButtonGroupIcon7" format="reference" />
        <attr name="ButtonGroupIcon8" format="reference" />
        <attr name="ButtonGroupIcon9" format="reference" />
        <attr name="ButtonGroupIcon10" format="reference" />
        <attr name="ButtonGroupText" format="string" />
    </declare-styleable>
    <declare-styleable name="DiagonalLabelView" >
        <attr name="android:text" />
        <attr name="android:color" />
        <attr name="BackgroundColor" />
        <attr name="Bold" />
        <attr name="DiagonalOrientation" format="enum" >
            <enum name="LEFT_TOP" value="1" />
            <enum name="RIGHT_TOP" value="2" />
            <enum name="LEFT_BOTTOM" value="3" />
            <enum name="RIGHT_BOTTOM" value="4" />
        </attr>
    </declare-styleable>
    <declare-styleable name="ExpandLayout" >
        <attr name="Duration" />
        <attr name="Expanded" />
        <attr name="Parallax" />
        <attr name="android:orientation" />
    </declare-styleable>
    <declare-styleable name="ImageSelectView" >
        <attr name="Column" />
        <attr name="MaxFileSize" />
        <attr name="MaxNum" />
        <attr name="Quality" />
    </declare-styleable>
    <declare-styleable name="InputView" >
        <attr name="android:maxLines" />
        <attr name="android:maxLength" />
        <attr name="android:inputType" />
        <attr name="android:textSize" />
    </declare-styleable>
    <declare-styleable name="LineView" >
        <attr name="android:color" />
    </declare-styleable>
    <declare-styleable name="NineGridView" >
        <attr name="Gap" />
    </declare-styleable>
    <declare-styleable name="PageTabView" >
        <attr name="android:textSize" />
    </declare-styleable>
    <declare-styleable name="PAGRefreshHeader" >
        <attr name="AssetName" />
    </declare-styleable>
    <declare-styleable name="SettingsItem" >
        <attr name="android:src" />
        <attr name="android:text" />
        <attr name="android:textColor" />
        <attr name="android:textSize" />
        <attr name="android:gravity" />
        <attr name="Bold" />
        <attr name="HasArrow" />
        <attr name="IconSize" />
        <attr name="Gap" />
    </declare-styleable>
    <declare-styleable name="TabView" >
        <attr name="android:textSize" />
    </declare-styleable>
    <declare-styleable name="TagView" >
        <attr name="android:textColor" />
        <attr name="ItemPaddingX" />
        <attr name="ItemPaddingY" />
        <attr name="Gap" />
    </declare-styleable>
    <declare-styleable name="ValueTextView" >
        <attr name="android:textSize" />
        <attr name="android:padding" />
        <attr name="Gap" />
        <attr name="ValueColor" />
        <attr name="TitleColor" />
        <attr name="Value" />
        <attr name="Title" />
    </declare-styleable>
</resources>