<?xml version="1.0" encoding="utf-8"?>
<resources>
    <attr name="BackgroundColor" format="color" />
    <attr name="BorderColor" format="color" />
    <attr name="TitleColor" format="color" />
    <attr name="ValueColor" format="color" />

    <attr name="BorderWidth" format="dimension" />
    <attr name="Gap" format="dimension" />
    <attr name="IconSize" format="dimension" >
        <enum name="wrap_content" value="-2" />
    </attr>
    <attr name="ItemPaddingX" format="dimension" />
    <attr name="ItemPaddingY" format="dimension" />

    <attr name="AssetName" format="string" />
    <attr name="Home" format="string" />
    <attr name="Title" format="string" />
    <attr name="Value" format="string" />

    <attr name="Duration" format="integer" />

    <attr name="Parallax" format="float" />
    <attr name="Ratio" format="float" />

    <attr name="Bold" format="boolean" />
    <attr name="Expanded" format="boolean" />
    <attr name="HasArrow" format="boolean" />

    <declare-styleable name="AspectRatioImageView" >
        <attr name="Ratio" />
    </declare-styleable>
    <declare-styleable name="AvatarView" >
        <attr name="BorderWidth" />
        <attr name="BorderColor" />
        <attr name="BackgroundColor" />
    </declare-styleable>
    <declare-styleable name="BreadCrumbView" >
        <attr name="Home" />
    </declare-styleable>
    <declare-styleable name="ExpandLayout" >
        <attr name="Duration" />
        <attr name="Expanded" />
        <attr name="Parallax" />
        <attr name="android:orientation" />
    </declare-styleable>
    <declare-styleable name="InputView" >
        <attr name="android:maxLines" />
        <attr name="android:maxLength" />
        <attr name="android:inputType" />
        <attr name="android:textSize" />
    </declare-styleable>
    <declare-styleable name="LineView" >
        <attr name="android:color" />
    </declare-styleable>
    <declare-styleable name="NineGridView" >
        <attr name="Gap" />
    </declare-styleable>
    <declare-styleable name="PageTabView" >
        <attr name="android:textSize" />
    </declare-styleable>
    <declare-styleable name="PAGRefreshHeader" >
        <attr name="AssetName" />
    </declare-styleable>
    <declare-styleable name="SettingsItem" >
        <attr name="android:src" />
        <attr name="android:text" />
        <attr name="android:textColor" />
        <attr name="android:textSize" />
        <attr name="android:gravity" />
        <attr name="Bold" />
        <attr name="HasArrow" />
        <attr name="IconSize" />
        <attr name="Gap" />
    </declare-styleable>
    <declare-styleable name="TabView" >
        <attr name="android:textSize" />
    </declare-styleable>
    <declare-styleable name="TagView" >
        <attr name="android:textColor" />
        <attr name="ItemPaddingX" />
        <attr name="ItemPaddingY" />
        <attr name="Gap" />
    </declare-styleable>
    <declare-styleable name="ValueTextView" >
        <attr name="android:textSize" />
        <attr name="android:padding" />
        <attr name="Gap" />
        <attr name="ValueColor" />
        <attr name="TitleColor" />
        <attr name="Value" />
        <attr name="Title" />
    </declare-styleable>
</resources>